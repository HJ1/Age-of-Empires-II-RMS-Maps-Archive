<TERRAIN_GENERATION>

create_terrain FOREST
{
     base_terrain GRASS
     spacing_to_other_terrain_types 5
     land_percent 25
     number_of_clumps 10
     clumping_factor 20
     set_avoid_player_start_areas
     set_scale_by_groups
}

